# Sample profile by groups keys

#fx cycle all 50 # Set horizontal wave effect with speed 

#Vars
var red ff0000
var darkorange ff4000
var orange ff8000
var salmon ffbf00
var yellow ffff00
var spring bfff00
var green1 80ff00
var green2 40ff00
var green3 00ff00
var green4 00ff40
var green5 00ff80
var mint 00ffbf
var cyan 00ffff
var blue1 00bfff
var blue2 0080ff
var blue3 0040ff
var blue4 0000ff
var blue5 4000ff
var violet 8000ff
var purple bf00ff
var pink ff00ff
var magenta ff00bf
var orchid ff0080
var pinkred ff0040
var white ffffff
var black 000000


# Line 1
k escape $darkorange
k f1 $orange
k f2 $salmon
k f3 $yellow
k f4 $spring

k f5 $green2
k f6 $green4
k f7 $green5
k f8 $mint

k f9 $blue1
k f10 $blue2
k f11 $blue4
k f12 $blue5

k print_screen $violet
k scroll_lock $purple
k pause $pink


# Line 2
